(0...<%= @processes %>).to_a.each do |num|
  God.watch do |w|
    w.group = 'queues'
    w.name = "queue-#{num}"
    w.dir = '<%= @basedir %>'
    w.uid = '<%= @user %>'
    w.gid = '<%= @group %>'
    w.log = "<%= @basedir %>/logs/queue-#{num}.log"
    w.start = 'bundle exec ruby queue.rb'
    w.env = {
      'LOG_LEVEL' => '<%= @log_level %>',
      'STDOUT_SYNC' => '<%= @stdout_sync %>'
    }
    w.keepalive
    w.stop_signal = 'INT'
  end
end
